e":"穿梭框练习.vue","sourceRoot":".","sourcesContent":["<template>\r\n<div class=\"container\">\r\n    <div class=\"box1\">\r\n        <div v-if=\"arr1.length > 0\">\r\n            <div v-for=\"(item,index) in arr1\" :key=\"item.id\">\r\n                <div @click=\"clickItem1(item,index)\" :class=\"{'active':activeIndex === index}\">{{item.name}}</div>\r\n            </div>\r\n        </div>\r\n        <div v-else>暂无数据</div>\r\n    </div>\r\n    <div class=\"center\">\r\n        <div @click=\"right\"> > </div>\r\n        <div @click=\"left\"> < </div>\r\n    </div>\r\n    <div class=\"box2\">\r\n        <div v-if=\"arr2.length > 0\">\r\n            <div v-for=\"(item,index) in arr2\" :key=\"item.id\">\r\n                <div @click = \"clickItem2(item, index)\" :class=\"{'active':activeIndex === index + arr1.length}\">{{item.name}}</div>\r\n            </div>\r\n        </div>\r\n        <div v-else>暂无数据</div>\r\n    </div>\r\n</div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name:'穿梭框练习',\r\n  data(){\r\n      return {\r\n          arr1: [\r\n          {\r\n            id: 1,\r\n            name: 'tom'\r\n          },\r\n          {\r\n            id: 2,\r\n            name: 'jack',\r\n          },\r\n          {\r\n            id: 3,\r\n            name: 'rose',\r\n          },\r\n        ],\r\n          arr2: [\r\n          {\r\n            id: 4,\r\n            name: 'sky'\r\n          },\r\n          {\r\n            id: 5,\r\n            name: 'alex'\r\n          },\r\n          {\r\n            id: 6,\r\n            name: 'jay'\r\n          },\r\n        ],\r\n          activeIndex : -1,\r\n          activeItem: {}\r\n      }\r\n  },\r\n    methods:{\r\n      clickItem1(item,index){\r\n          this.activeIndex = index\r\n          this.activeItem = item\r\n      },\r\n      clickItem2(item,index){\r\n          this.activeIndex = index + this.arr1.length\r\n          this.activeItem = item\r\n      },\r\n      left(){\r\n          if(this.activeIndex >= this.arr1.length && this.activeIndex > -1){\r\n              this.arr2.splice(this.activeIndex-this.arr1.length,1)\r\n              this.arr1.push(this.activeItem)\r\n              this.activeIndex = -1\r\n          }\r\n      },\r\n      right(){\r\n          if(this.activeIndex < this.arr1.length && this.activeIndex > -1){\r\n              this.arr1.splice(this.activeIndex,1)\r\n              this.arr2.push(this.activeItem)\r\n              this.activeIndex = -1\r\n          }\r\n      }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    .container {\r\n        display: flex;\r\n    }\r\n    .box1,.box2 {\r\n        width:200px;\r\n        height:400px;\r\n        border:1px solid #eee;\r\n        margin:0 40px;\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n    .box1 div,.box2 div {\r\n        margin: 5px 0;\r\n        cursor: pointer;\r\n    }\r\n    .active{\r\n        background: deepskyblue;\r\n    }\r\n    .center{\r\n        height:400px;\r\n        padding-top: 180px;\r\n    }\r\n    .center div {\r\n        cursor: pointer;\r\n        margin: 10px 0;\r\n    }\r\n</style>\r\n"]}]}